{
  "name": "sti3bas/laravel-scout-array-driver",
  "description": "Array driver for Laravel Scout",
  "keywords": [
    "array",
    "testing",
    "driver",
    "scout",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Laurynas Geigalas",
      "email": "sti3bas@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "laravel/scout": "^9.0",
    "illuminate/database": "^8.0|^9.0|^10.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "mockery/mockery": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Sti3bas\\ScoutArray\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sti3bas\\ScoutArray\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sti3bas\\ScoutArray\\ScoutArrayEngineServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
