{
    "name": "sti3bas/laravel-scout-array-driver",
    "description": "Array driver for Laravel Scout",
    "license": "MIT",
    "keywords": [
        "array",
        "testing",
        "driver",
        "scout",
        "laravel"
    ],
    "authors": [
        {
            "name": "Laurynas Geigalas",
            "email": "sti3bas@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1 || ^8.2",
        "illuminate/bus": "^9.0 || ^10.0 || ^11.0",
        "illuminate/contracts": "^9.0 || ^10.0 || ^11.0",
        "illuminate/database": "^9.0 || ^10.0 || ^11.0",
        "illuminate/http": "^9.0 || ^10.0 || ^11.0",
        "illuminate/pagination": "^9.0 || ^10.0 || ^11.0",
        "illuminate/queue": "^9.0 || ^10.0 || ^11.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "laravel/scout": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.5.1",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.0 || ^10.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Sti3bas\\ScoutArray\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sti3bas\\ScoutArray\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "11.x-dev"
        },
        "laravel": {
            "providers": [
                "Sti3bas\\ScoutArray\\ScoutArrayEngineServiceProvider"
            ]
        }
    }
}
